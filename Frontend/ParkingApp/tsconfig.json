{
    "compileOnSave": false,
    "compilerOptions": {
        "baseUrl": "./",
        "paths": {
            "@app": [
                "src/app"
            ],
            "@app/*": [
                "src/app/*"
            ],
            "@assets/*": [
                "assets/*"
            ],
            "@env": [
                "src/environments/environment.prod"
            ],
            "@pages/*": [
                "pages/*"
            ],
            "@core/*": [
                "app/core/*"
            ],
            "@services/*": [
                "app/services/*"
            ],
            "@theme/*": [
                "theme/*"
            ],
            "@shared/*": [
                "app/shared/*"
            ]
        },
        "outDir": "./dist/out-tsc",
        "sourceMap": true,
        "declaration": false,
        "downlevelIteration": true,
        "experimentalDecorators": true,
        "module": "esnext",
        "moduleResolution": "node",
        "importHelpers": true,
        "target": "es2015",
        "typeRoots": [
            "node_modules/@types",
            "node_modules/@types/jquery"
        ],
        "lib": [
            "es2018",
            "dom"
        ]
    },
    "angularCompilerOptions": {
        "fullTemplateTypeCheck": true,
        "strictInjectionParameters": true
    }
}
