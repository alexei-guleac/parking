{
    "name": "parking-app",
    "version": "0.0.2",
    "scripts": {
        "ng": "ng",
        "upd": "npm update",
        "start": "ng serve",
        "build": "ng build",
        "build-prod": "ng build --prod",
        "test": "ng test",
        "lint": "ng lint",
        "ts-fix": "tslint --fix --project ./tsconfig.json",
        "lint-fix": "ng lint --fix",
        "e2e": "ng e2e",
        "locale-init": "ngx-translate-extract --input ./src --output src/assets/i18n/en.json --key-as-default-value --replace --clean --sort --format namespaced-json",
        "locale-extract": "ngx-translate-extract -i ./src -o src/assets/i18n/*.json --clean --sort --format namespaced-json",
        "locale-extract-multiple": "ngx-translate-extract --input ./src --output src/assets/i18n/{en,ru,ro}.json --clean --format namespaced-json",
        "locale-update": "ngx-translate-extract -i ./src -o src/assets/i18n/*.json"
    },
    "private": true,
